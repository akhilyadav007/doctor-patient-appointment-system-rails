
<%= form_for @patient do |form| %>
    <p>
      <%= form.label :name%><br>
      <%= form.text_field :name %>
    </p>
    <p>
      <%= form.label :email %>
      <%= form.text_field  :email %>
    </p>
    <p>
    <%= form.label :age %>
    <%= form.text_field  :age %>
  </p>

  <p>
  <%= form.label :password %>
  <%= form.password_field  :password %>
</p>
    <p>
      <%= form.submit %>
    </p>
    <% end %>

    <br>

    <b>back to home page<%= link_to 'back', new_session_path %></b>